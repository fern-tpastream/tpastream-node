{
    "name": "@fern-api/tpastream",
    "version": "0.0.4",
    "repository": "https://github.com/fern-tpastream/tpastream-node",
    "main": "./index.js",
    "types": "./index.d.ts",
    "files": [
        "/api/**/*.{js,js.map,d.ts}",
        "/schemas/**/*.{js,js.map,d.ts}",
        "/core/**/*.{js,js.map,d.ts}",
        "/index.{js,js.map,d.ts}"
    ],
    "scripts": {
        "format": "prettier --write --print-width 120 '**/*.ts'",
        "build": "esbuild $(find . -name '*.ts' -not -path './node_modules/*') --format=cjs --sourcemap --outdir=. && tsc"
    },
    "dependencies": {
        "@types/url-join": "4.0.1",
        "axios": "^0.27.2",
        "url-join": "4.0.1"
    },
    "peerDependencies": {
        "axios": "*"
    },
    "devDependencies": {
        "@types/node": "17.0.33",
        "esbuild": "0.14.47",
        "prettier": "2.7.1",
        "typescript": "4.6.4"
    },
    "packageManager": "yarn@3.2.4"
}
